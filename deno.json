{
  "tasks": {
    "dev": "deno run --allow-read --allow-write --allow-env --allow-run --watch main.ts",
    "run": "deno run --allow-read --allow-write --allow-env --allow-run main.ts",
    "compile": "deno compile --allow-read --allow-write --allow-env --allow-run --output qn main.ts",
    "global-install": "deno compile --allow-read --allow-write --allow-env --allow-run --output qn main.ts && cp qn ~/.deno/bin/qn"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@std/path": "jsr:@std/path@1",
    "@std/fs": "jsr:@std/fs@1"
  }
}
